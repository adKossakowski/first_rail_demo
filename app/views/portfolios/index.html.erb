<h1>Portfolio Items</h1>

<%= link_to  "Create new Portfolio", new_portfolio_path %>

<% @portfolio_items.each do |item| %>
  <p><%= item.title %></p>
  <p><%= item.subtitle %></p>
  <p><%= item.body %></p>
  <p><%= item.main_image %></p>
  <p><%= item.thumb_image %></p>

  <%= link_to  "Edit", edit_portfolio_path(item.id) %>

<% end %>

